<div class="compare">
  {{#link-to "index" class="btn btn-primary btn-xs"}}back{{/link-to}}
  <h2>Comparing...</h2>
  <div class="row">
    {{#each model as |item|}}
      <div class="col-xs-6">
        <h3 class="text-center">{{item.display_location.full}}</h3>
        <p class="current-temp">{{item.temp_f}} &deg;F</p>
      </div>
    {{/each}}
  </div>
  <div class="row">
    <div class="col-xs-12">
      <p class="text-center">The Temperature difference between them is:<br/> <span class="current-temp">{{differenceInTemp}} &deg;F</span></p>
    </div>
  </div>
</div>
